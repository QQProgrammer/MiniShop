page {
  background-color: #efeef4;
}

.user {
  background-color: #efeef4;
  padding-bottom: 60rpx;
}

/* 第一部分 */

.user_head {
  background: url("data:image/png;base64,") no-repeat;
  width: 750rpx;
  height: 352rpx;
  background-size: 100% 100%;
  margin-bottom: 100rpx;
}

.user_head_first {
  height: 285rpx;
  /* background: linear-gradient(#f4a24b, #F2BC4D);  */
  display: flex;
  flex-direction: column;
  justify-content: center;
  color: #fff;
}

.user_head_first view {
  flex: 1;
  text-align: center;
}

.user_head_icon {
  height: 160rpx;
  width: 160rpx;
  border-radius: 80rpx;
  margin: 10rpx 30rpx;
}

.user_head_use text {
  display: block;
  margin-bottom: 10rpx;
}

.user_head_name {
  font-size: 28rpx;
}

.user_head_tel {
  font-size: 20rpx;
}

/* 第二部分 */

.user_bal_box {
  /* background: linear-gradient(#F2BC4D, #fff);  */
  padding: 0 30rpx;
}

.user_bal {
  height: 160rpx;
  background-color: #fff;
  border-radius: 20rpx;
  font-size: 20rpx;
  margin: 0 auto;
  display: flex;
  color: #333;
}

.user_bal_item {
  flex: 1;
  text-align: center;
}

.user_bal_item text {
  display: block;
  margin-top: 44rpx;
}

.user_bal_itemdesc {
  font-size: 40rpx;
  margin-top: 20rpx !important;
}

/* 第三部分 */

.user_icon_box {
  padding: 30rpx;
  position: relative;
}

.user_icon {
  background-color: #fff;
  border-radius: 20rpx;
}

.user_icon_item {
  display: flex;
  /* border-bottom: 1rpx solid #ccc; */
}

.item_item_no {
  border-bottom: none;
}

.item_size {
  margin: 30rpx 42rpx;
  flex: 1;
  text-align: center
}

.item_size_img {
  width: 88rpx;
  height: 88rpx;
  margin-left:30rpx;
}

.item_size_img image {
  width: 100%;
  height: 100%;
}

.item_size_text {
  margin-top: 10rpx;
  text-align: center;
  font-size: 20rpx;
}

.recharge {
  background: url("data:image/png;base64,") no-repeat;
  width: 320rpx;
  height: 92rpx;
  line-height: 92rpx;
  background-size: 100% 100%;
  position: absolute;
  bottom: -20rpx;
  right: -180rpx;
  z-index: 3;
  color: white;
  font-size: 28rpx;
  padding-left: 40rpx;
}
